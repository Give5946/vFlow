    <!-- Permissions -->
    <string name="permission_name_core">vFlow Core 服务</string>
    <string name="permission_desc_core">vFlow 的核心后台服务，用于执行模拟点击、系统操作等高级功能。可以通过 Shizuku 或 Root 启动。</string>
    <string name="permission_name_core_root">vFlow Core (Root)</string>
    <string name="permission_desc_core_root">需要 vFlow Core 以 Root 权限运行才能使用的功能。</string>
    <string name="permission_name_accessibility">无障碍服务</string>
    <string name="permission_desc_accessibility">实现自动化点击、查找、输入等核心功能所必需的权限。</string>
    <string name="permission_name_notifications">通知权限</string>
    <string name="permission_desc_notifications">用于显示Toast提示、发送任务结果通知等。</string>
    <string name="permission_name_overlay">悬浮窗权限</string>
    <string name="permission_desc_overlay">允许应用在后台执行时显示输入框等窗口，这是实现复杂自动化流程的关键。</string>
    <string name="permission_name_notification_listener">通知使用权</string>
    <string name="permission_desc_notification_listener">允许应用读取和操作状态栏通知，用于实现通知触发器、查找和移除通知等功能。</string>
    <string name="permission_name_storage">文件访问权限</string>
    <string name="permission_desc_storage">允许应用读写 /sdcard/vFlow 目录下的脚本和资源文件。</string>
    <string name="permission_name_sms">短信权限</string>
    <string name="permission_desc_sms">用于接收和读取短信，以触发相应的工作流。此权限组包含接收、读取短信。</string>
    <string name="permission_name_bluetooth">蓝牙权限</string>
    <string name="permission_desc_bluetooth">用于控制设备的蓝牙开关状态。</string>
    <string name="permission_name_write_settings">修改系统设置</string>
    <string name="permission_desc_write_settings">用于调整屏幕亮度等系统级别的设置。</string>
    <string name="permission_name_location">精确定位</string>
    <string name="permission_desc_location">在部分安卓版本上，获取已保存的Wi-Fi列表需要此权限。</string>
    <string name="permission_name_background_location">后台位置访问</string>
    <string name="permission_desc_background_location">需要在后台持续监听位置变化时使用此权限，例如位置触发器。</string>
    <string name="permission_name_shizuku">Shizuku</string>
    <string name="permission_desc_shizuku">允许应用通过 Shizuku 执行需要更高权限的操作，例如 Shell 命令。</string>
    <string name="permission_name_battery">后台运行权限</string>
    <string name="permission_desc_battery">将应用加入电池优化白名单，确保后台触发器（如按键监听）能长时间稳定运行。</string>
    <string name="permission_name_alarm">闹钟和提醒</string>
    <string name="permission_desc_alarm">用于"定时触发"功能，确保工作流可以在精确的时间被唤醒和执行。</string>
    <string name="permission_name_root">Root 权限</string>
    <string name="permission_desc_root">允许应用通过超级用户权限执行底层系统命令。</string>
    <string name="permission_name_usage_stats">使用情况访问</string>
    <string name="permission_desc_usage_stats">允许 vFlow 读取应用的使用统计信息（如使用时长、最后使用时间），提高 Agent 行为准确度。</string>
</resources>
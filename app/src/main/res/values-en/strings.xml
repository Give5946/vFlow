    <!-- Permissions -->
    <string name="permission_name_core">vFlow Core Service</string>
    <string name="permission_desc_core">vFlow Core background service for performing advanced operations like simulated clicks and system actions. Can be started via Shizuku or Root.</string>
    <string name="permission_name_core_root">vFlow Core (Root)</string>
    <string name="permission_desc_core_root">Features that require vFlow Core to run with Root privileges.</string>
    <string name="permission_name_accessibility">Accessibility Service</string>
    <string name="permission_desc_accessibility">Required for core automation features like clicks, find, and input.</string>
    <string name="permission_name_notifications">Notification Permission</string>
    <string name="permission_desc_notifications">Used to display Toast messages and send task result notifications.</string>
    <string name="permission_name_overlay">Overlay Permission</string>
    <string name="permission_desc_overlay">Allows the app to display input boxes and other windows while running in the background, essential for complex automation flows.</string>
    <string name="permission_name_notification_listener">Notification Listener</string>
    <string name="permission_desc_notification_listener">Allows the app to read and manipulate status bar notifications, used for notification triggers and notification management.</string>
    <string name="permission_name_storage">Storage Access</string>
    <string name="permission_desc_storage">Allows the app to read and write scripts and resource files in /sdcard/vFlow directory.</string>
    <string name="permission_name_sms">SMS Permission</string>
    <string name="permission_desc_sms">Used to receive and read SMS messages to trigger workflows. Includes receive and read SMS permissions.</string>
    <string name="permission_name_bluetooth">Bluetooth Permission</string>
    <string name="permission_desc_bluetooth">Used to control device Bluetooth on/off state.</string>
    <string name="permission_name_write_settings">Modify System Settings</string>
    <string name="permission_desc_write_settings">Used for system-level settings like screen brightness adjustment.</string>
    <string name="permission_name_location">Fine Location</string>
    <string name="permission_desc_location">Required on some Android versions to access saved Wi-Fi lists.</string>
    <string name="permission_name_background_location">Background Location Access</string>
    <string name="permission_desc_background_location">Required to continuously monitor location changes in the background, such as for location triggers.</string>
    <string name="permission_name_shizuku">Shizuku</string>
    <string name="permission_desc_shizuku">Allows the app to perform elevated operations through Shizuku, such as Shell commands.</string>
    <string name="permission_name_battery">Background Run Permission</string>
    <string name="permission_desc_battery">Add the app to battery optimization whitelist to ensure background triggers (like key listeners) run stably.</string>
    <string name="permission_name_alarm">Alarms and Reminders</string>
    <string name="permission_desc_alarm">Used for "Time Trigger" feature to ensure workflows can be awakened and executed at precise times.</string>
    <string name="permission_name_root">Root Permission</string>
    <string name="permission_desc_root">Allows the app to execute low-level system commands with superuser privileges.</string>
    <string name="permission_name_usage_stats">Usage Access</string>
    <string name="permission_desc_usage_stats">Allows vFlow to read app usage statistics (like usage duration, last used time) for improved Agent accuracy.</string>
